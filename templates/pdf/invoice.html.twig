<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% block stylesheets %}
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
        <style>
            @import url("https://fonts.googleapis.com/css2?family=Staatliches&display=swap");
            @import url("https://fonts.googleapis.com/css2?family=Nanum+Pen+Script&display=swap");

            * {
                margin: 0;
                padding: 0;
                box-sizing: border-box;
            }

            body,
            html {
                background: #f7f7f7;
                color: black;
                font-family: serif;
                font-size: 10px;
                letter-spacing: 0.1em;
                margin: 0;
                padding: 0;
                height: 100%;
            }

            body {
                text-align: center;
                padding-top: 120px;
            }

            strong {
                font-weight: 600;
            }

            h1 {
                font-size: 2rem;
            }

            h2 {
                color: #1F466F;
                font-size: 1.5rem;
                font-weight: 600;
            }

            table {
                margin: 0 auto;
                text-align: left;
                width: 700px;
                max-width: 700px;
            }

            td {
                padding: 0 10px;
            }

            th {
                text-align: left;
                padding: 10px;
            }

            tbody tr:first-of-type td,
            tbody tr:first-of-type th,
            tfoot tr:first-of-type td,
            tfoot tr:first-of-type th {
                padding-top: 20px;
            }

            tbody tr td,
            tfoot tr td {
                line-height: 1.5;
            }

            tfoot {
                font-weight: 600;
            }

            tfoot .total {
                font-size: 2rem;
            }

            thead td {
                vertical-align: top;
            }

            thead img {
                max-height: 100px;
            }

            thead .company-info {
                line-height: 2;
            }

            tr.subtitle td {
                padding-top: 20px;
                padding-bottom: 10px;
            }

            tr.detail td {
                line-height: 2;
            }

            tr.detail td:nth-of-type(1) {
                width: 400px;
            }

            tr.detail td:nth-of-type(2) {
                width: 150px;
            }

            tr.detail td:nth-of-type(3) {
                width: 150px;
            }

            tr.detail:nth-child(even) {
                background: #c4c4c4;
            }

            .text-center {
                text-align: center;
            }

            .text-right {
                text-align: right;
            }
        </style>
    {% endblock %}
</head>

<body translate="no">
<table id="main">
    <thead>
        <tr class="company-info">
            <td class="p-0"><strong>Asociación Tecnolóxiga A Industriosa</strong></td>
            <td rowspan="4" colspan="2" class="text-right">
                <img alt="A Industriosa" title="A Industriosa"
                     src="data:image/png;base64," />
            </td>
        </tr>
        <tr class="company-info">
            <td>NIF: G27846609</td>
        </tr>
        <tr class="company-info">
            <td>R/ Barcelona nº 13, Baixo Esquerda</td>
        </tr>
        <tr class="company-info">
            <td>Vigo</td>
        </tr>
    </thead>
    <tbody>
    <tr>
        <th colspan="3"><h1>Factura</h1></th>
    </tr>
    <tr>
        <th><h2>Facturar a:</h2></th>
        <th class="text-right"><h2>Factura #</h2></th>
        <th class="text-right"><h2>Fecha</h2></th>
    </tr>
    <tr>
        <td>
            {{ invoice.businessName }}
        </td>
        <td class="text-right">PCT23/{{ "%03d"|format(invoice.id) }}</td>
        <td class="text-right">{{ invoice.date|date("m/d/Y") }}</td>
    </tr>
    <tr>
        <td>CIF {{ invoice.cif }}</td>
        <td colspan="2"> </td>
    </tr>
    <tr>
        <td>{{ invoice.address }}</td>
        <td colspan="2"> </td>
    </tr>
    <tr>
        <td colspan="3"> </td>
    </tr>
    <tr>
        <td><strong>Contacto: </strong> {{ invoice.email }}</td>
        <td colspan="2"> </td>
    </tr>
    <tr>
        <td colspan="3"> </td>
    </tr>
    <tr class="subtitle">
        <td><h2>Descripción</h2></td>
        <td class="text-right"><h2>Cantidad</h2></td>
        <td class="text-right"><h2>Precio</h2></td>
    </tr>
    {% for item in details %}
    <tr class="detail">
        <td>{{ item.concept }}</td>
        <td class="text-right">1</td>
        <td class="text-right">{{ item.price|number_format(2) }} €</td>
    </tr>
    {% endfor %}
    {% for i in 0..blankLines %}
    <tr class="detail">
        <td colspan="3">&nbsp;</td>
    </tr>
    {% endfor %}
    </tbody>
    <tfoot>
        <tr class="subtotal">
            <td> </td>
            <td class="text-right">Subtotal</td>
            <td class="text-right">{{ global.subtotal|number_format(2) }} €</td>
        </tr>
        <tr class="iva">
            <td> </td>
            <td class="text-right">IVA 21 %</td>
            <td class="text-right">{{ global.tax|number_format(2) }} €</td>
        </tr>
        <tr class="total">
            <td colspan="3" class="text-right">{{ global.total|number_format(2) }} €</td>
        </tr>
    </tfoot>
</table>
</body>
</html>
